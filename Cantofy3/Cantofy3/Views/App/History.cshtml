<h1>Latest searches</h1>
<table class="table table-hover" ng-cloak>
    <tr ng-repeat="search in searches" ng-click="viewSearch(search.searchID)" style="cursor: pointer;">
        <td>
            <p><small class="pull-right" style="color:darkgray">{{search.date | date:'MMM d, y HH:mm'}}</small></p>
            <p>{{search.items}}</p>
            <p>{{search.romanization}}</p>
            <p>{{search.translation}}</p>
        </td>
    </tr>
</table>